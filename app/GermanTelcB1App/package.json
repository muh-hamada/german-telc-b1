    "build:android:apk:german-b2": "./build-android.sh german-b2 apk",
    "build:android:apk:english-b1": "./build-android.sh english-b1 apk",
    "build:android:apk:english-b2": "./build-android.sh english-b2 apk",
    "build:ios:german-b1": "./build-ios.sh german-b1",
    "build:ios:german-b2": "./build-ios.sh german-b2",
