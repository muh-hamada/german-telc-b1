    "ios:pods": "cd ios && rm -rf build && rm -rf ~/Library/Developer/Xcode/DerivedData/TelcExamApp-* && pod install && cd ..",
    "ios:german-b1": "./scripts/build-config.sh german-b1 ios && react-native run-ios --scheme TelcExamApp",
    "ios:german-b2": "./scripts/build-config.sh german-b2 ios && react-native run-ios --scheme TelcExamApp",
    "ios:english-b1": "./scripts/build-config.sh english-b1 ios && react-native run-ios --scheme TelcExamApp",
